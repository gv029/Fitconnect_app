<h1>Fill Out Your Profile</h1>


<div class='jumbotron'>
  <div class='row'>
    <div class='col-md-6'>
      <div class='thumbnail'>
        <img src='...' alt='...'>
        <div class='caption'>
          <h3>Upload Profile Pic</h3>
          <p>.....</p>
          <p><a href='#' class='btn btn-primary' role='button'>Upload Pic</a></p>
        </div>
      </div><!--END THUMBNAIL-->
    </div><!--END COL-->
    <div class='col-md-6'>
      <div class='thumbnail'>
        <img src='...' alt='...'>
        <div class='caption'>
          <h3>Upload Your Clients Before/After Pics</h3>
          <p>.....</p>
          <p><a href='#' class='btn btn-primary' role='button'>Upload Pic</a></p>
        </div>
      </div>
    </div>
  <div class='row'>
    <div class='col-md-6'>  
        <%= form_for @users, html: { class: 'form-inline'} do |f| %>
    <div class='form-group' >
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div class='form-group' >
        <%= f.label :bio %>
        <%= f.text_area :bio %>
    </div>
      
    
    
  </div><!--END Col-->
  <div class='col-md-6'>
    <p>Years Experience</p>
  <div class='form-group'>
    <p>Number Of Certifications</p>
    <%= f.label :certifications %>
    <%= f.number_field :certifications %>
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
    <% end %>
  </div>
    
  </div><!--END COL-->
</div><!--END ROW-->
</div><!--Close Jumbotron -->
