<h1>This the index page for trainer</h1>

<% @users.each do |user| %>

<div class='row'>
 
    <div class='span0 well text-center'>
     
        <i class='icon-arrow-up'></i>
      <br/>
      
     
        <i class='icon-arrow-down'></i>
      
    </div>

      <div class='span8 well'>
        
        <h4><%= link_to user.name %></h4>
        <p><%= user.bio %></p>
        <p><%= user.email %></p>
        <p><%= user.score %></p>
        <p><%= user.certifications %></p>
        <p>
          <span class='quiet'>posted by</span>  
          
        </p>
     
      </div> 

</div>

<% end %>
